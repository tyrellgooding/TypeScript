{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": false, //Allows or disallows use of the any type.
        "sourceMap": false, //Allows for app.ts to show in console on chrome - allows you to debug.
        "strictNullChecks": true,
        "noEmitOnError": false, // Stops JS file from compiling if false.
        "noUnusedParameters": false // Checks if any parameters are not being used.
    },
    "exclude": [
        "node_modules"
    ]
}